# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.IndexController.index

GET     /printers                   controllers.PrintersController.index
GET     /printers/list              controllers.PrintersController.list

GET     /cats/create                controllers.NewCatsController.createTable()
GET     /cats/list                  controllers.NewCatsController.findAll
GET     /cats/find/:string          controllers.NewCatsController.find(string)
POST    /cats/insert                controllers.NewCatsController.insertOrUpdate()
GET     /cats/:pk                   controllers.NewCatsController.findOne(pk)



GET     /asset-groups/create        controllers.Asset_GroupController.createTable()
GET     /asset-groups/list          controllers.Asset_GroupController.findAll
GET     /asset-groups/find/:string  controllers.Asset_GroupController.find(string)
POST    /asset-groups/insert        controllers.Asset_GroupController.insertOrUpdate()
GET     /asset-groups/:PK           controllers.Asset_GroupController.findOne(PK)



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
